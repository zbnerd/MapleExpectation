      "datasource": "Prometheus",
      "description": "Heap memory usage percentage",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "green",
                  "text": "0-60%"
                },
                "1": {
                  "color": "yellow",
                  "text": "60-80%"
                },
                "2": {
                  "color": "red",
                  "text": "80-100%"
                }
              },
              "type": "value"
            }
          ],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 80
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          },
          "unit": "percent"
