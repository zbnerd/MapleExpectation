{
  "dashboard": {
    "title": "MapleExpectation - Before Refactoring",
    "tags": ["maple", "refactoring", "baseline"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "5s",
    "panels": [
      {
        "id": 1,
        "title": "Module Size (Files)",
        "type": "gauge",
        "targets": [
          {
            "expr": "module_files_total{module=\"app\"}",
            "legendFormat": "module-app"
          },
          {
            "expr": "module_files_total{module=\"infra\"}",
            "legendFormat": "module-infra"
          },
          {
            "expr": "module_files_total{module=\"core\"}",
            "legendFormat": "module-core"
          },
          {
            "expr": "module_files_total{module=\"common\"}",
            "legendFormat": "module-common"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "min": 0,
            "max": 400,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 150, "color": "yellow"},
                {"value": 300, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Configuration Class Distribution",
        "type": "piechart",
        "targets": [
          {
            "expr": "config_classes_total{module=\"app\"}",
            "legendFormat": "module-app (56 bloated)"
          },
          {
            "expr": "config_classes_total{module=\"infra\"}",
            "legendFormat": "module-infra (0)"
          }
        ]
      },
      {
        "id": 3,
        "title": "Module Dependency Violations",
        "type": "stat",
        "targets": [
          {
            "expr": "dependency_violations{type=\"circular\"}",
            "legendFormat": "Circular Dependencies"
          },
          {
            "expr": "dependency_violations{type=\"spring_in_core\"}",
            "legendFormat": "Spring in Core"
          },
          {
            "expr": "dependency_violations{type=\"reverse\"}",
            "legendFormat": "Reverse Dependencies"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "Build Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(build_duration_seconds{phase=\"compile\"}[5m])",
            "legendFormat": "Compile Time"
          },
          {
            "expr": "rate(build_duration_seconds{phase=\"test\"}[5m])",
            "legendFormat": "Test Time"
          }
        ]
      },
      {
        "id": 5,
        "title": "Test Pass Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(test_count{status=\"passed\"}[1h]) / rate(test_count[1h]) * 100",
            "legendFormat": "Pass Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "max": 100,
            "min": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 95, "color": "yellow"},
                {"value": 99, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Architecture Compliance Score",
        "type": "gauge",
        "targets": [
          {
            "expr": "architecture_compliance_score",
            "legendFormat": "Compliance"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 70, "color": "yellow"},
                {"value": 90, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Stateless Design Compliance",
        "type": "gauge",
        "targets": [
          {
            "expr": "stateless_compliance_score",
            "legendFormat": "Stateless %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 90, "color": "green"}
              ]
            }
          }
        }
      }
    ]
  },
  "overwrite": true,
  "message": "Before Refactoring Baseline - module-app has 342 files (56% bloated) with 56 config classes"
}
